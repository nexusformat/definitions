<definition xmlns="http://definition.nexusformat.org/nxdl/3.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" category="base" type="group" name="NXcs_mm_obj" extends="NXobject" xsi:schemaLocation="http://definition.nexusformat.org/nxdl/3.1 ../nxdl.xsd">
    <symbols>
        <doc>
             The symbols used in the schema to specify e.g. dimensions of arrays.
        </doc>
    </symbols>
    <doc>
         Computer science description of a memory in a memory system.
    </doc>
    <field name="technology" type="NX_CHAR">
        <doc>
             Qualifier for the type of random access memory.
        </doc>
    </field>
    <!--make an enumeration-->
    <field name="max_physical_capacity" type="NX_NUMBER" units="NX_ANY">
        <doc>
             Total amount of data which the medium can hold.
        </doc>
    </field>
    <!--NX_BIT-->
    <field name="name" type="NX_CHAR">
        <doc>
             Given name to the I/O unit.
        </doc>
    </field>
    <group type="NXfabrication"/>
</definition>
