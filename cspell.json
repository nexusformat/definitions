{
    "version": "0.2",
    "ignorePaths": [
        "./tests/data/*",
        "*.toml",
        "Makefile",
        "*.bat",
        "*.egg-info"
    ],
    "dictionaryDefinitions": [
        {
            "name": "custom-dictionary",
            "path": "./.cspell/custom-dictionary.txt",
            "addWords": true
        }
    ],
    "import": ["@cspell/dict-scientific-terms-us/cspell-ext.json"],
    "dictionaries": [
        "custom-dictionary",
        "scientific_terms_US",
        "python"
    ],
    "words": [],
    "ignoreWords": [],
    "ignoreRegExpList": ["\\bNX\\w*\\b", "https?://[\\S]+"],
    "language": "en-US"
}